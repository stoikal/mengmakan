{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle":"off",
      "class-methods-use-this": "off",
      "no-console": "warn",
      "no-unused-vars": "warn",
      "import/no-extraneous-dependencies": [
        "error", 
        {
          "devDependencies": true
        }
      ]
    }
}
